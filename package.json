{
  "name": "tab-homepage",
  "description": "Homepage for Tab for a Cause (https://tab.gladly.io)",
  "version": "1.0.0",
  "author": "The Tab for a Cause Team",
  "dependencies": {
    "@material-ui/icons": "^1.0.0-beta.43",
    "@reach/router": "^1.1.1",
    "babel-polyfill": "^6.26.0",
    "browser-detect": "^0.2.26",
    "common-tags": "^1.8.0",
    "gatsby": "^2.5.3",
    "gatsby-link": "^2.1.1",
    "gatsby-plugin-facebook-pixel": "^1.0.3",
    "gatsby-plugin-google-analytics": "^2.0.20",
    "gatsby-plugin-jss": "^2.0.9",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-sitemap": "^2.1.0",
    "gatsby-plugin-typography": "^2.2.13",
    "gatsby-source-filesystem": "^2.0.36",
    "gatsby-transformer-yaml": "^2.1.12",
    "lodash": "^4.17.10",
    "material-ui": "^1.0.0-beta.41",
    "mdi-material-ui": "^4.0.0",
    "moment": "^2.22.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-slick": "^0.23.1",
    "react-transition-group": "^2.3.1",
    "react-typography": "^0.16.19",
    "slick-carousel": "^1.8.1",
    "typography": "^0.16.19"
  },
  "keywords": [
    "tab",
    "cause",
    "extension",
    "charity"
  ],
  "license": "MPL-2.0",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "eslint --fix 'src/**/*.js'",
    "lint": "eslint 'src/**/*.js'",
    "serve": "gatsby serve",
    "test": "yarn run lint && jest --env=jsdom",
    "test:watch": "jest --env=jsdom --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.8.0",
    "jsdom": "^15.1.0",
    "prettier": "^1.17.1",
    "react-test-renderer": "^16.8.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|dist|docs|public|node_modules|.cache)[/\\\\]"
    ],
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  }
}
