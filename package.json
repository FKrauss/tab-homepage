{
  "name": "tab-homepage",
  "description": "Homepage for Tab for a Cause (https://tab.gladly.io)",
  "version": "1.0.0",
  "author": "The Tab for a Cause Team",
  "dependencies": {
    "browser-detect": "^0.2.26",
    "gatsby": "^1.9.247",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-jss": "^1.5.12",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-typography": "^1.7.18",
    "material-ui": "^1.0.0-beta.41",
    "react-helmet": "^5.2.0"
  },
  "keywords": ["tab", "cause", "extension", "charity"],
  "license": "MPL-2.0",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "eslint --fix 'src/**/*.js'",
    "lint": "eslint 'src/**/*.js'",
    "test": "yarn run lint && jest --env=jsdom"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.3",
    "jsdom": "^11.7.0",
    "prettier": "^1.11.1",
    "react-test-renderer": "^15.5.0"
  },
  "jest": {
    "collectCoverageFrom": ["src/**/*.{js,jsx}"],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|dist|docs|public|node_modules|.cache)[/\\\\]"
    ],
    "testEnvironment": "node",
    "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"]
  }
}
