<RoutingRules>

  <!-- Zendesk (use temporary redirects) -->

  <RoutingRule>
    <Condition>
      <KeyPrefixEquals>more/contact/</KeyPrefixEquals>
    </Condition>
    <Redirect>
      <ReplaceKeyWith>contact/</ReplaceKeyWith>
      <HostName>tab.gladly.io</HostName>
      <Protocol>https</Protocol>
      <HttpRedirectCode>301</HttpRedirectCode>
    </Redirect>
  </RoutingRule>

  <RoutingRule>
    <Condition>
      <KeyPrefixEquals>help/</KeyPrefixEquals>
    </Condition>
    <Redirect>
      <ReplaceKeyWith>hc/en-us/categories/201939608-Tab-for-a-Cause/</ReplaceKeyWith>
      <HostName>gladly.zendesk.com</HostName>
      <Protocol>https</Protocol>
      <HttpRedirectCode>307</HttpRedirectCode>
    </Redirect>
  </RoutingRule>


